{
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "allowJs": true,
    "checkJs": false,
    "noEmit": false,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "module": "ESNext",
    "resolveJsonModule": true,
    "sourceMap": false
  },
  "include": [
    "helper"
  ],
  "exclude": [
    "node_modules",
  ],
  "files": [
    "main.ts",
  ]
}
